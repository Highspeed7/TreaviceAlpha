{
  "extends": "tslint:recommended",
  "rules": {
    "no-trailing-whitespace": false,
    "trailing-comma": [
      true,
      {
        "multiline": "never",
        "singleline": "never"
      }
    ],
    "object-literal-sort-keys": false,
    "object-literal-key-quotes": false,
    "no-reference": false,
    "max-line-length": [ false, 120 ],
    "align": [ false, "parameters", "statements" ]
  }
}